{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/api/health.js" },
    { "src": "/favicon\\.(?:ico|png)$", "dest": "/api/health.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ],
  "functions": {
    "api/index.js":  { "maxDuration": 10, "memory": 1024 },
    "api/health.js": { "maxDuration": 5,  "memory": 128 }
  }
}
